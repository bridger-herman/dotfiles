'.source.c':
  'Multiline Comment':
    'prefix': 'mcm'
    'body': """
/*
 *
 */
"""
  'Debug':
    'prefix': 'db'
    'body': """
#ifdef DEBUG

#endif
"""

'.text.tex.latex':
  'Typewriter Text':
    'prefix': 'tt'
    'body': '\\\\texttt{${1:text}}'
  'Italics':
    'prefix': 'em'
    'body': '\\\\emph{${1:text}}'
